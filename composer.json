{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability" : "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "qz/qzphp",
                "version": "0.1.10",
                "source": {
                    "url": "https://github.com/fendy3002/QzPhp.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {"QzPhp\\": "src/QzPhp"}
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "coco/foundation",
                "version": "0.3.3",
                "source": {
                    "url": "ssh://git@git.coderscolony.co:2268/Libraries/LaravelFoundation.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Coco\\": "src/Coco"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "map/core",
                "version": "0.1.4",
                "source": {
                    "url": "ssh://git@git.coderscolony.co:2268/HotelMapper/core.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {"MapCore\\": "src/MapCore"}
                }
            }
        }
    ],

    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "qz/qzphp": "0.1.10",
        "coco/foundation": "0.3.3",
        "map/core": "0.1.4",
        "predis/predis" : "~1.0",
        "maatwebsite/excel": "~2.0.0",
        "alexpechkarev/google-maps": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.3",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
